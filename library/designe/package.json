  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@types/classnames": "2.3.1",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "cross-env": "7.0.3",
    "resolve": "1.22.0"
  },
  "babel": {
    "presets": [
      [
