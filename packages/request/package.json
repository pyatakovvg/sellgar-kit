{
  "name": "@package/request",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "create-d.ts": "tsc",
    "build": "rm -drf ./lib && cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files && npm run create-d.ts",
    "start:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "dependencies": {
    "@helper/utils": "0.0.1",
    "axios": "0.26.1",
    "qs": "6.10.3"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@types/react": "18.0.1",
    "cross-env": "7.0.3",
    "resolve": "1.22.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  }
}
