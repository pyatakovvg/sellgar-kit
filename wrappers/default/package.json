{
  "name": "@wrapper/default",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "create-d.ts": "tsc",
    "build": "rm -drf ./lib && cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files && npm run create-d.ts",
    "start:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "peerDependencies": {
    "react": "18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@types/react": "18.0.5",
    "cross-env": "7.0.3",
    "resolve": "1.22.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  }
}
