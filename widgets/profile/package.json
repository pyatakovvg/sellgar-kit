{
  "private": true,
  "name": "@widget/profile",
  "version": "0.0.1",
  "author": {
    "name": "Viktor Pyatakov",
    "email": "viktor.pyatakov@pimpay.ru"
  },
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "create-d.ts": "tsc",
    "build": "rm -drf ./lib && cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files && npm run create-d.ts",
    "start:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "peerDependencies": {
    "@helper/utils": "0.0.1",
    "@reduxjs/toolkit": "1.8.1",
    "package/request": "0.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.0",
    "core-js": "3.21.1",
    "cross-env": "7.0.3",
    "resolve": "1.22.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  }
}
